   "source": [
    "def get_base_acc(y): # Calculating the base accuracy (using the median)\n",
    "    predictions = [np.median(y)] * len(y)\n",
    "    return accuracy_score(y, predictions)\n",
    "\n",
    "\n",
    "def k_fold_acc(X, y, classifier, kfold): # Performing a k-fold cross validation (k=5)\n",
    "    scores = cross_validate(classifier, X, y, scoring = 'accuracy', cv = kfold)\n",
    "    return np.mean(scores['test_score']) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "c33ae92f",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-08-21T19:05:45.383831Z",
     "iopub.status.busy": "2024-08-21T19:05:45.383331Z",
     "iopub.status.idle": "2024-08-21T19:05:45.395496Z",
     "shell.execute_reply": "2024-08-21T19:05:45.394230Z"
    },
    "id": "1AUKnsOn-Qdl",
    "papermill": {
     "duration": 0.070935,
     "end_time": "2024-08-21T19:05:45.398260",
     "exception": false,
     "start_time": "2024-08-21T19:05:45.327325",
     "status": "completed"
